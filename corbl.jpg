  --border-radius: 8px;
  position: fixed;
  right: 0;
  bottom: 0;
  background: var(--background_content);
  box-shadow: 0 1px 0 var(--separator_alternate),
    0 0 0 1px var(--separator_alternate), 0 0 2px rgba(0, 0, 0, 0.08),
    0 8px 24px rgba(0, 0, 0, 0.08);
  border-radius: var(--border-radius) var(--border-radius) 0 0;
  min-height: 120px;
  display: flex;
  flex-direction: column;
  animation: FCWindow__in 0.2s ease;
}

.FCWindow--blocked::after {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: inherit;
  background: transparent;
}

.FCWindow--active {
  z-index: 1;
}

.FCWindow__header {
  display: flex;
  align-items: center;
  height: 44px;
  border-bottom: 1px solid var(--separator_alternate);
  border-radius: 8px 8px 0 0;
  flex: 0 0 44px;
}

/**
 * Повышаем специфичность, чтобы перебить 011 ([dir] h1) в vkcom
 */
.FCWindow__title.FCWindow__title {
  margin: 0;
  padding: 10px;
  flex: 1 1 auto;
  font: 500 13px/16px var(--font);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  cursor: grab;
  user-select: none;
  color: var(--text_primary);
}

.FCWindow--dragging .FCWindow__title {
  cursor: grabbing;
}

.FCWindow__actions {
  flex: 0 0 auto;
  padding-right: 6px;
}

/* change to gap when we drop safari < 14 support */
.FCWindow__actions > :nth-child(n + 2) {
  margin-left: 4px;
}

.FCWindow__content {
  display: flex;
  flex-direction: column;
  flex: 1 1 auto;
  overflow: auto;
  overscroll-behavior: none;
}

.FCWindow__footer {
  flex-shrink: 0;
  position: relative;
  display: flex;
  align-items: center;
  border-top: 1px solid var(--separator_alternate);
}

.FCWindow--rounded {
  border-radius: var(--border-radius);
}

@keyframes FCWindow__in {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}

